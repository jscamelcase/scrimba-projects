{
  "manifest_version": 3,
  "name": "URL Saver",
  "version": "1.0",
  "description": "Save URLs of visited pages to a list.",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon48.png"
    }
  }
}
